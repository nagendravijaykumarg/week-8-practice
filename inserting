 import java.util.*;
public class Insert{
  public static void main(String[] args){
Scanner sc=new Scanner(System.in);
    
    ArrayList<Integer> al=new ArrayList<Integer>();
    for(int i=0;i<7;i++){    
      System.out.println("Enter first number");
    int a=sc.nextInt();
     al.add(a);
    }    
    System.out.println(al);
for(Integer o: al){
  if(o%2==0){
    System.out.println(o);
  }
}
    
    
}
}
